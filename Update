local function ZQIW_fake_script() -- OpenH.SoundEffect 
	local script = Instance.new('LocalScript', OpenH)

	function PlaySound( )
		script.Parent.Click:Play( )
	end
	script.Parent.MouseButton1Click:connect(PlaySound)
end
