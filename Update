local function RMDJSJV_fake_script() -- OpenH.RotationEffect 
	local script = Instance.new('LocalScript', OpenH)

	script.Parent.MouseEnter:Connect(function()
		for i = 0,-5,-5 do
			wait()
			script.Parent.Rotation = i
		end
	end)
	
	script.Parent.MouseLeave:Connect(function()
		for i = -5,0,5 do
			wait()
			script.Parent.Rotation = i
		end
	end)
end
coroutine.wrap(RMDJSJV_fake_script)()
